<!-- Add more parking spots as needed -->
<script>
// You can use JavaScript to update the parking spot
status dynamically
// For simplicity, here's a sample code to toggle the
status of a spot
document.querySelectorAll('.parking-spot').forEach(functio n(spot) {
spot.addEventListener('click', function() { if (spot.classList.contains('vacant')) {
spot.classList.remove('vacant');
spot.classList.add('occupied'); } else {
spot.classList.remove('occupied');
spot.classList.add('vacant'); }
}); });
</script> </body>
